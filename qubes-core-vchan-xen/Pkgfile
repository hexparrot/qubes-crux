name=qubes-core-vchan-xen
version=4.1.7
release=1
source=(https://github.com/QubesOS/qubes-core-vchan-xen/archive/refs/tags/v${version}.tar.gz)

build() {
	cd $name-$version
	make all
	make DESTDIR=$PKG install
}
